{% extends 'MascaEtudiantBundle::admin-lycee.html.twig' %}
{% block title %}Créer grille ecolage lycée{% endblock %}
{% block admin_lycee_body %}
    {% if error_message is defined %}
        <p style="color: red "> {{ error_message }}</p>
    {% endif %}
    <form method="post">
        {{ form_widget(grilleForm) }}
        <div class="align-right">
            <input type="submit" value="Enregistre" class="btn">
            <a href="{{ path('grille_ecolage_lycee') }}" class="btn">Annuler</a>
        </div>
    </form>
{% endblock %}