{% extends "MascaEtudiantBundle::admin-lycee.html.twig" %}
{% block admin_lycee_body %}
    <h5>Ajout/modification matierer emploi du temps lycée</h5>
    {{ jour }} à {{ heure }}
    <form method="post">
        {{ form_widget(form) }}
        <div class="submit-style">
            <div class="floatleft">
                <a href="{{ path('emploi_du_temps_lycee',{'id':classeId}) }}">
                    <img title="Annuler" src="{{ asset('assets/cancel.png') }}">
                </a>

            </div>
            <div class="floatright">
                <input type="image" src="{{ asset('assets/save.png') }}" title="Enregitrer">
            </div>
        </div>
    </form>
{% endblock %}