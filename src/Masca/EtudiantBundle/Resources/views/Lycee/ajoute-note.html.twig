{% extends 'MascaEtudiantBundle::layout.html.twig' %}
{% block title %}Ajouter note{% endblock %}
{% block etudiant_body %}
    <div id="DivInfo" style="display:none;">
        <p style="color: red ">Matiere déjà existant</p>
    </div>
    {% if (js is defined) %}{{ js|raw }}{% endif %}
    <h2>Note de {{ lyceen.person.prenom }} {{ lyceen.person.nom }}</h2>
    <form method="post">
        {{ form_widget(form) }}
        <div class="align-right">
            <input type="submit" value="Ajouter" class="btn">
            <a href="{{ path('liste_notes_lyceen',{'lyceen_id':lyceen.id}) }}" class="btn">Annuler</a>
        </div>
    </form>
{% endblock %}